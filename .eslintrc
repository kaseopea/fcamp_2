{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "globals": {
    "process": true,
    "window": true,
    "document": true,
    "fetch": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "comma-dangle": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}